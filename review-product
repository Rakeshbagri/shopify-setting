<script>
  $(".product__details-content h2").html(function(){
    var text= $(this).text().trim().split(" ");
    var first = text.shift();
    return (text.length > 0 ? "<span class='first-word'>"+ first + "</span> " : first) + text.join(" ");
  });
</script>
